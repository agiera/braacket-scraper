"url = 'SOURCE_URL'\nresponse = requests.get(url)\nsoup = BeautifulSoup(response.content, 'html.parser')\ntournaments = []\ntournament_tags = soup.find_all('TAG_FOR_TOURNAMENT')\nfor tournament_tag in tournament_tags:\ndata = {}\nid = tournament_tag['ID_ATTRIBUTE_FOR_TOURNAMENT']\ndata['id'] = id\nurl = tournament_tag.find('TAG_FOR_URL')['href']\ndata['url'] = url\nname = tournament_tag.find('TAG_FOR_NAME').text\ndata['name'] = name\ndate = tournament_tag.find('TAG_FOR_DATE').text\ndata['date'] = date\ncountry = tournament_tag.find('TAG_FOR_COUNTRY').text\ndata['country'] = country\nregion = tournament_tag.find('TAG_FOR_REGION').text\ndata['region'] = region\nnumber_of_players = int(tournament_tag.find('TAG_FOR_NUMBER_OF_PLAYERS').text)\ndata['number_of_players'] = number_of_players\ntournaments.append(data)\nreturn json.dumps(tournaments, indent=4)"